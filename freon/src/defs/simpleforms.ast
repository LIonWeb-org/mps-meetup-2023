language simpleforms

model MainModel {
    name: identifier;
    forms: Form[];
}

interface INamedConcept {
    name: identifier;
}

modelunit Form  {
    name: identifier;
    groups: ItemGroup[];
}

concept ItemGroup implements INamedConcept {
    items: SimpleItem[];
}

concept SimpleItem implements INamedConcept {
    type: Type;
}

abstract concept Type {}

concept StringType base Type {
}

concept IntType base Type {
}
